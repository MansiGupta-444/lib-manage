
<% layout("/layouts/boilarplate") %>
 <h3 class="fw-bold text-primary mb-3"> All Books</h3>

 <div class="row row-cols-lg-4 row-cols-md-2 row-cols-sm-1">
 <% for (let lists of list) { %>
      <a href="/library/<%=lists._id%>" class="listing-link">
     <div class="card col listing-card" >
  <img src="<%= lists.image.url %>" class="card-img-top" alt="listing image" >
  <div class="card-img-overlay"></div>
  <div class="card-body">
    <p class="card-text">
    <b><%= lists.title %> <br></b>
    </p>
  </div>
</div>
</a>
<%  } %>
  </div>


     
